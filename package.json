{
  "name": "@brlt/n",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Nicholas Berlette <nick@berlette.com>",
  "contributors": [
    "Anthony Fu <anthonyfu117@hotmail.com>",
    "Nicholas Berlette <nick@berlette.com>"
  ],
  "bugs": "https://github.com/brlt/n/issues",
  "homepage": "https://github.com/brlt/n#readme",
  "readme": "https://github.com/brlt/n#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brlt/n.git"
  },
  "description": "Use the right package manager",
  "keywords": [
    "pnpm",
    "yarn",
    "npm",
    "cli"
  ],
  "packageManager": "pnpm@6.23.8",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "tsup": {
    "entry": [
      "./src/index.ts",
      "./src/n*.ts"
    ],
    "platform": "node",
    "outDir": "dist",
    "format": [
      "cjs",
      "esm"
    ],
    "onSuccess": "esno scripts/postbuild.ts",
    "dts": true,
    "sourcemap": true,
    "minify": true,
    "clean": true
  },
  "scripts": {
    "prepublishOnly": "pnpm clean && pnpm build",
    "watch": "pnpm build -- --watch",
    "build": "tsup",
    "release": "pnpx bumpp --commit 'ðŸ“¦ v%s' --push --tag",
    "lint": "eslint --ignore-path=.gitignore \"**/*.ts\"",
    "lint:fix": "pnpm lint -- --fix",
    "format:base": "prettier --ignore-path=.gitignore --plugin-search-dir=. '**/*.ts'",
    "format:check": "pnpm format:base --check",
    "format": "pnpm format:base --write",
    "test": "vitest",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist bin node_modules",
    "?ni": "pnpm install",
    "ni": "esno src/cmd/ni.ts",
    "nci": "esno src/cmd/nci.ts",
    "nr": "esno src/cmd/nr.ts",
    "nm": "esno src/cmd/nm.ts",
    "nex": "esno src/cmd/nex.ts",
    "nun": "esno src/cmd/nun.ts",
    "dev": "esno src/cmd/ni.ts",
    "nu": "esno src/cmd/nu.ts",
    "no": "esno src/cmd/no.ts",
    "nv": "esno src/cmd/nv.ts",
    "nd": "esno src/cmd/nd.ts",
    "nh": "esno src/cmd/nh.ts",
    "na": "esno src/cmd/na.ts",
    "nl": "esno src/cmd/nl.ts",
    "nls": "esno src/cmd/nls.ts",
    "nt": "esno src/cmd/nt.ts",
    "np": "esno src/cmd/np.ts",
    "nb": "esno src/cmd/nb.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.20.3",
    "@antfu/utils": "^0.5.1",
    "@brlt/prettier": "^1.6.4",
    "@types/ini": "^1.3.31",
    "@types/node": "^17.0.24",
    "@types/prompts": "^2.0.14",
    "@types/which": "^2.0.1",
    "eslint": "^8.13.0",
    "esm": "^3.2.25",
    "esno": "^0.14.1",
    "execa": "^6.1.0",
    "find-up": "^6.3.0",
    "fs-extra": "^10.1.0",
    "fsxx": "^0.0.5",
    "ini": "^3.0.0",
    "pnpm": "^6.32.8",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "terminal-link": "^3.0.0",
    "tsm": "^2.2.1",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "vitest": "^0.9.3",
    "which": "^2.0.2",
    "zx": "^6.1.0"
  }
}
